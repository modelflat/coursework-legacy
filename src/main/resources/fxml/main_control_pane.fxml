<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" cache="true" cacheHint="SPEED" depthTest="DISABLE" maxHeight="-Infinity"
            maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" pickOnBounds="false" prefHeight="600.0"
            prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.112"
            fx:controller="com.github.modelflat.coursework2.ui.MainControlPaneController">
    <VBox layoutX="10.0" layoutY="10.0" prefHeight="504.0" prefWidth="300.0">
        <VBox cache="true" cacheHint="SPEED" pickOnBounds="false" prefHeight="152.0" prefWidth="300.0">
            <CheckBox fx:id="doCLClearCheckBox" mnemonicParsing="false" onMouseClicked="#doCLClearCheckBoxClicked"
                      selected="true" text="Perform CL clear">
                <VBox.margin>
                    <Insets bottom="10.0"/>
                </VBox.margin>
            </CheckBox>
            <CheckBox fx:id="doPostClearCheckBox" mnemonicParsing="false" onMouseClicked="#doPostClearCheckBoxClicked"
                      selected="true" text="Perform statistical clear">
                <VBox.margin>
                    <Insets bottom="10.0"/>
                </VBox.margin>
            </CheckBox>
            <CheckBox fx:id="doRecomputeFractalCheckBox" mnemonicParsing="false"
                      onMouseClicked="#doRecomputeFractalCheckBoxClicked" text="Recompute fractal">
                <VBox.margin>
                    <Insets bottom="10.0"/>
                </VBox.margin>
            </CheckBox>
            <FlowPane prefHeight="200.0" prefWidth="200.0">
                <Label text="work items:">
                    <padding>
                        <Insets left="4.0"/>
                    </padding>
                </Label>
                <TextField fx:id="workItemsTextField" prefHeight="25.0" prefWidth="64.0"/>
                <Label text="run count:">
                    <padding>
                        <Insets left="4.0"/>
                    </padding>
                </Label>
                <TextField fx:id="runCountTextField" prefHeight="25.0" prefWidth="64.0">
                    <FlowPane.margin>
                        <Insets left="4.0"/>
                    </FlowPane.margin>
                </TextField>
                <Label text="iter count:">
                    <padding>
                        <Insets left="4.0"/>
                    </padding>
                    <FlowPane.margin>
                        <Insets right="7.0"/>
                    </FlowPane.margin>
                </Label>
                <TextField fx:id="iterCountTextField" prefHeight="25.0" prefWidth="64.0"/>
                <Label text="skip count:">
                    <padding>
                        <Insets left="4.0"/>
                    </padding>
                </Label>
                <TextField fx:id="skipCountTextField" prefHeight="25.0" prefWidth="64.0"/>
                <Button fx:id="applyRunSettingsButton" mnemonicParsing="false"
                        onMouseClicked="#applyRunSettingsButtonClicked" prefHeight="25.0" prefWidth="129.0"
                        text="Apply">
                    <FlowPane.margin>
                        <Insets bottom="4.0" top="4.0"/>
                    </FlowPane.margin>
                </Button>
            </FlowPane>
            <opaqueInsets>
                <Insets bottom="5.0"/>
            </opaqueInsets>
        </VBox>
        <VBox fx:id="metaEvolutionPaneVBox" prefHeight="364.0" prefWidth="300.0">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
            <CheckBox fx:id="doEvolveCheckBox" mnemonicParsing="false" onMouseClicked="#doEvolveCheckBoxClicked"
                      selected="true" text="Evolve"/>
            <VBox fx:id="evolutionPaneVBox">
                <padding>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                </padding>
                <!-- T evolution controls -->
                <HBox alignment="BASELINE_LEFT">
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <VBox prefWidth="180.0">
                        <CheckBox fx:id="doEvolveTCheckBox" mnemonicParsing="false"
                                  onMouseClicked="#doEvolveTCheckBoxClicked" text="t"/>
                        <Label fx:id="tCurrentStateLabel" text="t = ?">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Label>
                    </VBox>
                    <Button fx:id="tEvolutionCustomizeButton" mnemonicParsing="false"
                            onMouseClicked="#tEvolutionCustomizeButtonClicked" text="Customize...">
                        <HBox.margin>
                            <Insets right="10.0"/>
                        </HBox.margin>
                    </Button>
                </HBox>
                <Slider fx:id="tSlider"/>
                <!-- C real -->
                <HBox alignment="BASELINE_LEFT">
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <VBox prefWidth="180.0">
                        <CheckBox fx:id="doEvolveCRealCheckBox" mnemonicParsing="false"
                                  onMouseClicked="#doEvolveCRealCheckBoxClicked" text="C Real"/>
                        <Label fx:id="cRealCurrentStateLabel" prefHeight="20.0" prefWidth="183.0" text="cReal = ?">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Label>
                    </VBox>
                    <Button fx:id="cRealEvolutionCustomizeButton" mnemonicParsing="false"
                            onMouseClicked="#cRealEvolutionCustomizeButtonClicked" text="Customize...">
                        <HBox.margin>
                            <Insets right="10.0"/>
                        </HBox.margin>
                    </Button>
                </HBox>
                <Slider fx:id="cRealSlider"/>
                <!-- C imaginary -->
                <HBox alignment="BASELINE_LEFT">
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <VBox prefWidth="180.0">
                        <HBox.margin>
                            <Insets/>
                        </HBox.margin>
                        <CheckBox fx:id="doEvolveCImagCheckBox" mnemonicParsing="false"
                                  onMouseClicked="#doEvolveCImagCheckBoxClicked" text="C Imaginary"/>
                        <Label fx:id="cImagCurrentStateLabel" text="cImag = ?">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Label>
                    </VBox>
                    <Button fx:id="cImagEvolutionCustomizeButton" mnemonicParsing="false"
                            onMouseClicked="#cImagEvolutionCustomizeButtonClicked" text="Customize...">
                        <HBox.margin>
                            <Insets right="10.0"/>
                        </HBox.margin>
                    </Button>
                </HBox>
                <Slider fx:id="cImagSlider"/>
                <!-- Bounds evolution -->
                <VBox prefHeight="216.0" prefWidth="370.0">
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                    <CheckBox fx:id="doEvolveBoundsCheckBox" mnemonicParsing="false"
                              onMouseClicked="#doEvolveBoundsCheckBoxClicked" text="Bounds">
                        <VBox.margin>
                            <Insets bottom="8.0"/>
                        </VBox.margin>
                    </CheckBox>
                    <Slider fx:id="boundsSlider" max="10.0"/>
                    <VBox fx:id="boundsEvolutionPaneVBox" prefHeight="117.0" prefWidth="360.0">
                        <HBox alignment="BASELINE_LEFT" prefHeight="20.0" prefWidth="390.0">
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </padding>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <CheckBox fx:id="doEvolveXMinCheckBox" mnemonicParsing="false"
                                      onMouseClicked="#doEvolveXMinCheckBoxClicked" text="X min">
                                <HBox.margin>
                                    <Insets right="10.0"/>
                                </HBox.margin>
                            </CheckBox>
                            <Button mnemonicParsing="false" onMouseClicked="#xMinEvolutionCustomizeButtonClicked"
                                    text="Customize..." fx:id="xMinEvolutionCustomizeButton">
                                <HBox.margin>
                                    <Insets right="10.0"/>
                                </HBox.margin>
                            </Button>
                            <Label text="xMin = ?" fx:id="xMinCurrentStateLabel">
                                <font>
                                    <Font size="14.0"/>
                                </font>
                            </Label>
                        </HBox>
                        <HBox alignment="BASELINE_LEFT" prefHeight="20.0" prefWidth="390.0">
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </padding>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <CheckBox fx:id="doEvolveXMaxCheckBox" mnemonicParsing="false"
                                      onMouseClicked="#doEvolveXMaxCheckBoxClicked" text="X max">
                                <HBox.margin>
                                    <Insets right="8.0"/>
                                </HBox.margin>
                            </CheckBox>
                            <Button mnemonicParsing="false" onMouseClicked="#xMaxEvolutionCustomizeButtonClicked"
                                    text="Customize..." fx:id="xMaxEvolutionCustomizeButton">
                                <HBox.margin>
                                    <Insets right="10.0"/>
                                </HBox.margin>
                            </Button>
                            <Label text="xMax = ?" fx:id="xMaxCurrentStateLabel">
                                <font>
                                    <Font size="14.0"/>
                                </font>
                            </Label>
                        </HBox>
                        <HBox alignment="BASELINE_LEFT" prefHeight="20.0" prefWidth="390.0">
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </padding>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <CheckBox fx:id="doEvolveYMinCheckBox" mnemonicParsing="false"
                                      onMouseClicked="#doEvolveYMinCheckBoxClicked" text="Y min">
                                <HBox.margin>
                                    <Insets right="11.0"/>
                                </HBox.margin>
                            </CheckBox>
                            <Button fx:id="yMinEvolutionCustomizeButton" mnemonicParsing="false"
                                    onMouseClicked="#yMinEvolutionCustomizeButtonClicked" text="Customize...">
                                <HBox.margin>
                                    <Insets right="10.0"/>
                                </HBox.margin>
                            </Button>
                            <Label fx:id="yMinCurrentStateLabel" text="yMin = ?">
                                <font>
                                    <Font size="14.0"/>
                                </font>
                            </Label>
                        </HBox>
                        <HBox alignment="BASELINE_LEFT" prefHeight="20.0" prefWidth="390.0">
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </padding>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <CheckBox fx:id="doEvolveYMaxCheckBox" mnemonicParsing="false"
                                      onMouseClicked="#doEvolveYMaxCheckBoxClicked" text="Y max">
                                <HBox.margin>
                                    <Insets right="10.0"/>
                                </HBox.margin>
                            </CheckBox>
                            <Button fx:id="yMaxEvolutionCustomizeButton" mnemonicParsing="false"
                                    onMouseClicked="#yMaxEvolutionCustomizeButtonClicked" text="Customize...">
                                <HBox.margin>
                                    <Insets right="10.0"/>
                                </HBox.margin>
                            </Button>
                            <Label fx:id="yMaxCurrentStateLabel" text="yMax = ?">
                                <font>
                                    <Font size="14.0"/>
                                </font>
                            </Label>
                        </HBox>
                    </VBox>
                </VBox>


            </VBox>
        </VBox>
    </VBox>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</AnchorPane>
